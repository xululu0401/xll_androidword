<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xululu.aidlmoduleservice">

    <permission android:name="com.xululu.aidlmoduleservice.permission.ACCESS_BOOK_SERVICE"
        android:protectionLevel="normal"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.xululu.aidlmoduleservice.MessengerService"
            android:exported="true"/>

        <service android:name="com.xululu.aidlmoduleservice.aidlservice.BookManagerService"
            android:exported="true">
        </service>

        <service android:name=".aidlservice.BindPoolService"
            android:exported="true"/>

    </application>

</manifest>